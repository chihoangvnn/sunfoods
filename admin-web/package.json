  "scripts": {
    "dev": "npx vite",
    "build": "npx vite build",
    "preview": "npx vite preview",
    "validate-tailwind": "npm run build && node -e \"const fs=require('fs'); const css=fs.readdirSync('dist/assets').find(f=>f.endsWith('.css')); const content=fs.readFileSync('dist/assets/'+css,'utf8'); const hasUtils=content.includes('.flex{')&&content.includes('.grid{'); console.log(hasUtils?'✅ Tailwind utilities found':'❌ No utilities - check config'); process.exit(hasUtils?0:1)\"",
    "clean-install": "rm -R node_modules 2>/dev/null; rm package-lock.json 2>/dev/null; npm install",
    "build:check": "npm run build && npm run validate-tailwind",
    "build:verify": "npm run build && ls -lh dist/assets/*.css && echo '---' && echo 'Checking Tailwind utilities...' && grep -o '\\.flex{' dist/assets/*.css | wc -l | xargs -I {} echo 'Found {} .flex utilities'"
  },
