run = "cd customer-tramhuong && npm install && npm run dev -- -p 3002 -H 0.0.0.0"
modules = ["nodejs-20"]

[nix]
channel = "stable-24_05"

[deployment]
run = ["sh", "-c", "cd customer-tramhuong && npm install && npm start"]

[[ports]]
